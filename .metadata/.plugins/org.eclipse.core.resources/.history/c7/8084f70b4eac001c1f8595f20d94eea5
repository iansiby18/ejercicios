int main(void) {
	setbuf(stdout,NULL);
	int numeroingresado;
	int acumulador=0;
for(int i=0; i<5;i++){
	printf("Ingrese un numero entero: ");
	scanf("%d",&numeroingresado);

	acumulador= acumulador+numeroingresado;
}

printf("el acumulador termino en: %d ",acumulador);
	return EXIT_SUCCESS;
}
