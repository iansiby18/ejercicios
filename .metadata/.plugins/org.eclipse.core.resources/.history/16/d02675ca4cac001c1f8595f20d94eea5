int main(void) {
	setbuf(stdout,NULL);
	int suma;
	int numeroingresado;
	int numeroingresado2;
	int resultado;
	printf("ingrese un numero:\n");
	scanf("%d", &numeroingresado);
	printf("ingrese otro numero:\n");
	scanf("%d", &numeroingresado2);

	resultado = suma;
	printf("El resultado de la division es: %d", resultado);

	int suma(int num1,int num2){
		num1+num2;
	}

	return EXIT_SUCCESS;
